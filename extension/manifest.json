{
  "manifest_version": 2,
  "name": "Zoom Web",
  "version": "1.2",
  "description": "Opens zoom calls on web client",
  "content_scripts": [
    {
      "matches": ["*://*.zoom.us/j/*", "*://*.zoom.us/s/*"],
      "js": ["zoom-web.js"]
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "tanner@tellen.me"
    }
  }
}
